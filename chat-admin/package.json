{
    "name": "chat-admin",
    "version": "1.0.0",
    "description": "Chat Admin Backend - Customer Service Management System",
    "main": "server.js",
    "scripts": {
        "start": "node server.js",
        "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
        "dev:backend": "nodemon server.js",
        "dev:frontend": "npm run --prefix frontend dev",
        "build": "npm run --prefix frontend build",
        "test": "jest",
        "test:watch": "jest --watch",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "migrate": "node scripts/migrate.js",
        "seed": "node scripts/seed.js",
        "setup:frontend": "mkdir -p frontend && cp frontend-package.json frontend/package.json && cd frontend && npm install"
    },
    "keywords": [
        "chat",
        "customer-service",
        "admin",
        "backend",
        "api"
    ],
    "author": "Chat Admin Team",
    "license": "MIT",
    "dependencies": {
        "@ant-design/icons": "^6.0.0",
        "@ant-design/pro-components": "^2.8.9",
        "@ant-design/pro-layout": "^7.22.6",
        "@tanstack/react-query": "^5.80.7",
        "antd": "^5.26.0",
        "axios": "^1.9.0",
        "bcrypt": "^5.1.1",
        "compression": "^1.7.4",
        "concurrently": "^9.1.2",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^7.1.5",
        "express-validator": "^7.0.1",
        "helmet": "^7.1.0",
        "ioredis": "^5.3.2",
        "joi": "^17.11.0",
        "jsonwebtoken": "^9.0.2",
        "lodash": "^4.17.21",
        "moment": "^2.29.4",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.6.5",
        "react-router-dom": "^6.30.1",
        "uuid": "^9.0.1",
        "winston": "^3.11.0",
        "winston-daily-rotate-file": "^4.7.1",
        "ws": "^8.14.2",
        "zustand": "^5.0.5"
    },
    "devDependencies": {
        "@types/jest": "^29.5.8",
        "eslint": "^8.55.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-n": "^16.4.0",
        "eslint-plugin-promise": "^6.1.1",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "supertest": "^6.3.3"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": ">=8.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/your-org/chat-admin.git"
    },
    "bugs": {
        "url": "https://github.com/your-org/chat-admin/issues"
    },
    "homepage": "https://github.com/your-org/chat-admin#readme"
}
